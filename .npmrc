
# Enhanced npm configuration for GamePath AI development
legacy-peer-deps=true
engine-strict=false
package-lock=true
save-exact=true
fund=false
audit=false
strict-ssl=false
registry=https://registry.npmjs.org/
loglevel=error
fetch-retries=5
fetch-retry-factor=2
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
# Added to ensure package-lock.json is generated on install
package-lock-only=false
